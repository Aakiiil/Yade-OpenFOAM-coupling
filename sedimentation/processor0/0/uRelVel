FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      uRelVel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    y1
    {
        type            cyclic;
    }
    y0
    {
        type            cyclic;
    }
    x0
    {
        type            cyclic;
    }
    x1
    {
        type            cyclic;
    }
    z0
    {
        type            cyclic;
    }
    z1
    {
        type            cyclic;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to1throughx0
    {
        type            processorCyclic;
        value           uniform (0 0 0);
    }
}


