FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      uSourceDrag;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    y1
    {
        type            cyclic;
    }
    y0
    {
        type            cyclic;
    }
    x0
    {
        type            cyclic;
    }
    x1
    {
        type            cyclic;
    }
    z0
    {
        type            cyclic;
    }
    z1
    {
        type            cyclic;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to0throughx1
    {
        type            processorCyclic;
        value           uniform 0;
    }
}


